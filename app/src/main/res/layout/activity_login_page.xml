<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Login_page"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="1dp">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo__1_" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="Go Meals"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textviewsignup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="Get Food  Delivered to Your\n Doorstep"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="Login To Your Account"
        android:textSize="23dp"
        app:layout_constraintEnd_toEndOf="@+id/textviewsignup"
        app:layout_constraintStart_toStartOf="@+id/textviewsignup"
        app:layout_constraintTop_toBottomOf="@+id/textviewsignup" />

    <EditText
        android:id="@+id/Loginemail_text_signup"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="22dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/edittext_shape"
        android:drawableLeft="@drawable/mail_icon"
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:paddingLeft="15dp"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/Password_signup"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="22dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/edittext_shape"
        android:drawableLeft="@drawable/black_lock_icon"
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textEmailAddress"
        android:paddingLeft="15dp"
        app:layout_constraintEnd_toEndOf="@+id/Loginemail_text_signup"
        app:layout_constraintStart_toStartOf="@+id/Loginemail_text_signup"
        app:layout_constraintTop_toBottomOf="@+id/Loginemail_text_signup" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="or"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/Password_signup"
        app:layout_constraintStart_toStartOf="@+id/Password_signup"
        app:layout_constraintTop_toBottomOf="@+id/Password_signup" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Continue With"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/facebooklogingpage"
        android:layout_width="152dp"
        android:layout_height="57dp"
        android:layout_margin="20dp"
        android:layout_marginStart="60dp"
        android:drawableLeft="@drawable/facebook_3_1"
        android:text="Facebook"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.079" />

    <Button
        android:id="@+id/Googlesignuppage"
        android:layout_width="152dp"
        android:layout_height="57dp"
        android:layout_margin="20dp"
        android:layout_marginEnd="60dp"
        android:drawableLeft="@drawable/google_icon_1"
        android:text="Google"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.079" />

        <Button
            android:id="@+id/Loginbuttonsignuppage"
            android:layout_width="152dp"
            android:layout_height="60dp"
            android:layout_marginBottom="50dp"
            android:text="Login"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/Googlesignuppage"
            app:layout_constraintStart_toStartOf="@+id/facebooklogingpage"
            app:layout_constraintTop_toBottomOf="@+id/facebooklogingpage" />

    <TextView
        android:id="@+id/sentsignup_loginpage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="10dp"
        android:text="Don't have an account"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/Loginbuttonsignuppage"
        app:layout_constraintStart_toStartOf="@+id/Loginbuttonsignuppage"
        app:layout_constraintTop_toBottomOf="@+id/Loginbuttonsignuppage" />

</androidx.constraintlayout.widget.ConstraintLayout>